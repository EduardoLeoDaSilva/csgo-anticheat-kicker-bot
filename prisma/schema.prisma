// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model PlayersAntiCheating {
  SteamId  String @id 
  Name String @map("UserId")
  IsConnected  Boolean @map("Email")
  Map  String @map("Robot")
  LastPhotoTaken  DateTime @map("RobotId")
  IsAntiCheatOpen  Boolean @map("RobotIdName")
  Expiration  DateTime @map("StopWin")
}
